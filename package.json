{
  "name": "music-marketplace-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@fontsource/inter": "^5.2.6",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.10.0",
    "firebase": "^11.10.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-firebase-hooks": "^5.1.1",
    "react-router-dom": "^7.6.2",
    "uuid": "^11.1.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "swa:build": "npm run build",
    "swa:start": "swa start build --api-devserver-url http://localhost:5000 --port 4280",
    "swa:dev": "swa start http://localhost:5173 --api-devserver-url http://localhost:5000 --port 4280",
    "start:local": "concurrently \"npm run dev\" \"npm run swa:dev\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "tailwindcss": "^4.1.11",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
